<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include layout="@layout/head_title_bar" />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
<LinearLayout
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="wrap_content">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="90dp"
            android:layout_height="match_parent"
            android:background="@color/color_eff1fd"
            android:gravity="left|center_vertical"
            android:paddingBottom="@dimen/distance_10dp"
            android:paddingLeft="@dimen/distance_15dp"
            android:paddingTop="@dimen/distance_10dp"
            android:text="@string/superversion_superversion_name"
            android:textColor="@color/textcolor"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/tv_value_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="left|center_vertical"
            android:paddingBottom="@dimen/distance_10dp"
            android:paddingLeft="@dimen/distance_10dp"
            android:paddingRight="@dimen/distance_10dp"
            android:paddingTop="@dimen/distance_10dp"
            android:textColor="@color/textcolor"
            android:textSize="16sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1.5dp"
        android:background="@color/grey_line" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="90dp"
            android:layout_height="match_parent"
            android:background="@color/color_eff1fd"
            android:gravity="left|center_vertical"
            android:paddingBottom="@dimen/distance_10dp"
            android:paddingLeft="@dimen/distance_15dp"
            android:paddingTop="@dimen/distance_10dp"
            android:text="@string/superversion_feed_back_percent"
            android:textColor="@color/textcolor"
            android:textSize="16sp" />

        <EditText
            android:id="@+id/tv_value_percent"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:inputType="number"
           android:padding="@dimen/distance_10dp"
            android:background="#00000000"
            android:textColor="@color/textcolor"
            android:textSize="16sp" />
        <TextView
            android:layout_width="90dp"
            android:layout_height="match_parent"
            android:gravity="left|center_vertical"
            android:paddingBottom="@dimen/distance_10dp"
            android:paddingLeft="@dimen/distance_15dp"
            android:paddingTop="@dimen/distance_10dp"
            android:text="%"
            android:textColor="@color/textcolor"
            android:textSize="16sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1.5dp"
        android:background="@color/grey_line" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="90dp"
            android:layout_height="match_parent"
            android:background="@color/color_eff1fd"
            android:gravity="left|center_vertical"
            android:paddingBottom="@dimen/distance_10dp"
            android:paddingLeft="@dimen/distance_15dp"
            android:paddingTop="@dimen/distance_10dp"
            android:text="@string/superversion_feed_back_start_time"
            android:textColor="@color/textcolor"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/tv_value_start_time"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="left|center_vertical"
            android:paddingBottom="@dimen/distance_10dp"
            android:paddingLeft="@dimen/distance_10dp"
            android:paddingRight="@dimen/distance_10dp"
            android:text="请选择"
            android:paddingTop="@dimen/distance_10dp"
            android:textColor="@color/textcolor"
            android:textSize="16sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1.5dp"
        android:background="@color/grey_line" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="90dp"
            android:layout_height="match_parent"
            android:background="@color/color_eff1fd"
            android:gravity="left|center_vertical"
            android:paddingBottom="@dimen/distance_10dp"
            android:paddingLeft="@dimen/distance_15dp"
            android:paddingTop="@dimen/distance_10dp"
            android:text="@string/superversion_feed_back_end_time"
            android:textColor="@color/textcolor"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/tv_value_end_time"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="left|center_vertical"
            android:paddingBottom="@dimen/distance_10dp"
            android:paddingLeft="@dimen/distance_10dp"
            android:paddingRight="@dimen/distance_10dp"
            android:text="请选择"
            android:paddingTop="@dimen/distance_10dp"
            android:textColor="@color/textcolor"
            android:textSize="16sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1.5dp"
        android:background="@color/grey_line" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/width_80dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="90dp"
            android:layout_height="match_parent"
            android:background="@color/color_eff1fd"
            android:gravity="left|center_vertical"
            android:paddingBottom="@dimen/distance_10dp"
            android:paddingLeft="@dimen/distance_15dp"
            android:paddingTop="@dimen/distance_10dp"
            android:text="@string/superversion_feed_back_content"
            android:textColor="@color/textcolor"
            android:textSize="16sp" />

        <EditText
            android:id="@+id/tv_value_content"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="left|top"
            android:background="#00000000"
           android:padding="@dimen/distance_10dp"
            android:textColor="@color/textcolor"
            android:textSize="16sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1.5dp"
        android:background="@color/grey_line" />

</LinearLayout>

        <TextView
            android:id="@+id/tv_confirm"
            android:layout_width="match_parent"
            android:layout_height="@dimen/distance_50dp"
            android:layout_gravity="bottom"
            android:background="@color/department_color"
            android:gravity="center"
            android:text="@string/approval_refuse_issuse_confirm"
            android:textColor="@color/white"
            android:textSize="@dimen/font_size_18sp"
            android:textStyle="bold" />
    </FrameLayout>
</LinearLayout>